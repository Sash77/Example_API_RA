{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://kofax.com/simpleRecord",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "type",
    "editable"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "RECORD"
      ],
      "pattern": "^(.*)$"
    },
    "editable": {
      "$id": "#/properties/editable",
      "type": "boolean",
      "title": "The Editable Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "url": {
      "$id": "#/properties/url",
      "type": "string",
      "title": "The Url Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "note": {
      "$id": "#/properties/note",
      "type": "string",
      "title": "The Note Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "creatorName": {
      "$id": "#/properties/creatorName",
      "type": "string",
      "title": "The Creatorname Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "mimeType": {
      "$id": "#/properties/mimeType",
      "type": "string",
      "title": "The Mimetype Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "sapMimeType": {
      "$id": "#/properties/sapMimeType",
      "type": "string",
      "title": "The Sapmimetype Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "messages": {
      "$id": "#/properties/messages",
      "type": "array",
      "title": "The Messages Schema",
      "items": {
        "$id": "#/properties/messages/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      }
    },
    "attributes": {
      "$id": "#/properties/attributes",
      "type": "array",
      "title": "The Attributes Schema",
      "items": {
        "$id": "#/properties/attributes/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      }
    },
    "tables": {
      "$id": "#/properties/tables",
      "type": "array",
      "title": "The Tables Schema",
      "items": {
        "$id": "#/properties/tables/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "string"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}